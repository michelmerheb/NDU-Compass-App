{
  "expo": {
    "name": "NDU Compass",
    "slug": "ndu-compass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/NDUCompassLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/NDUCompassLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#005eb8"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/NDUCompassLogo.png",
        "backgroundColor": "#005eb8"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.michelmerheb.nducompass"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "91f2b0f3-8910-45fa-992b-fda4db4d0759"
      }
    }
  }
}
